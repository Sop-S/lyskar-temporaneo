#===================================================#



extends Spatial

onready var player_pack = preload(\"res://instances/player.tscn\")
onready var cam_pack    = preload(\"res://instances/cam.tscn\")
onready var mouse_raycaster = load(\"res://script/mouse_raycaster.gd\")

var player
var cam
var raycaster

#--- var temporanee
var angolo_camera_globale

func _ready():
	initialize_game()

func initialize_game():
	player = player_pack.instance()
	cam    = cam_pack.instance()
	
	raycaster = mouse_raycaster.new(player.get_node(\"cam_follow/cam\"))
	
	add_child(player)
	player.global_transform.origin = $spawn.global_transform.origin
	


func _on_dead_drop_body_entered(body):
	if body == player:
		player.global_transform.origin = $spawn.global_transform.origin

func _process(delta):
#	$GUI/cont.fl_player_is_on_floor = player.fl_is_on_floor
	pass


func _physics_process(delta):
	if Input.is_action_just_pressed(\"ClickAction\"):
		var result: Dictionary = raycaster.cast_ray(get_viewport().get_mouse_position(), get_world())
		print(result)
	
	
	
	
	
	
	
"

